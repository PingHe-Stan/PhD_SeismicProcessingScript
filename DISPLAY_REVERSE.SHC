! display traces in order of station code and component Z,N,E by auto reading of readQ.sh
! PH 2015.10
!
sdef cnt 1
sdef ntr
calc i &ntr = $dsptrcs
hide all
disp_start:
  if "cnt gti "ntr    goto/forward disp_exit:
  display _recno("cnt)
  calc i &cnt = "cnt + 1
  goto disp_start:
disp_exit:
rd
return
